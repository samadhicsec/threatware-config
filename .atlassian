[DEFAULT]
# Uncomment one section below to configure credentials for accessing Confluence

# Uncomment this section for Confluence Cloud
#url = https://<replace with URL of your Confluence Cloud server>
#username = <replace with username>
#api_token = <replace with api_token>
#cloud = True

# Uncomment this section for Confluence Server with PAT
#url = <replace with your Confluence Server URL>
#token = <replace with your Confluence Server Personal Access Token>
#cloud = False

# Uncomment this section for Conlfuence with Username and Password (not recommended)
#url = <replace with your Confluence Server URL>
#username = <replace with your Confluence Server username>
#password = <replace with your Confluence Server password>
#cloud = False
